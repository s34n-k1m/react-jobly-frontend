m}\n * */\nfunction Routes({ currentUser, logout }) {\n  return (\n    <>\n      <Nav currentUser={currentUser} logout={logout}/>\n      <Switch>\n        <Route exact path=\"/\">\n          <Homepage />\n        </Route>\n        <Route exact path=\"/companies\">\n          <CompanyList />\n        </Route>\n        <Route exact path=\"/companies/:handle\">\n          <CompanyDetail />\n        </Route>\n        <Route exact path=\"/jobs\">\n          <JobList />\n        </Route>\n        <Route exact path=\"/login\">\n          <LoginForm />\n        </Route>\n        <Route exact path=\"/signup\">\n          <SignupForm />\n        </Route>\n        <Route exact path=\"/profile\">\n          <ProfileForm />\n        </Route>\n        <Redirect to=\"/\"/>\n      </Switch>\n    </>\n  );\n\n}\n\nexport default Routes;","/Users/seankim/Dropbox/Programming/Rithm/rithm-exercises/w9/react-jobly-frontend/src/Homepage.js",[],"/Users/seankim/Dropbox/Programming/Rithm/rithm-exercises/w9/react-jobly-frontend/src/Nav.js",[],"/Users/seankim/Dropbox/Programming/Rithm/rithm-exercises/w9/react-jobly-frontend/src/CompanyDetail.js",["73"],"import JoblyApi from \"./api\";\nimport { useState, useEffect } from \"react\";\nimport { useParams, Redirect } from \"react-router-dom\";\nimport JobCard from \"./JobCard\";\n\n/*  \nProps: none\nParams: handle\nState: company {handle, name, ...}\nApp -> Routes -> CompanyDetail\n*/\nfunction CompanyDetail() {\n  const { handle } = useParams();\n  const [company, setCompany] = useState(null);\n  const [isCompanyNotFound, setIsCompanyNotFound] = useState(false);\n\n  /* get company */\n  useEffect(function getCompany() {\n    async function getCompanyWithApi() {\n      try {\n        const resCompany = await JoblyApi.getCompany(handle);\n        console.log(\"RES COMPANY=\", resCompany);\n        setCompany(resCompany);\n      } catch (err) {\n        //have a state for company not found, true and false\n        setIsCompanyNotFound(true);\n      }\n    }\n    getCompanyWithApi();\n  }, [handle]);\n  \n  if (isCompanyNotFound === true) return <Redirect to=\"/\" />\n  if (!company) return <div>Loading...</div>;\n\n  return (\n    <div className=\"CompanyDetail\">\n      <h4 className=\"CompanyDetail-name text-left\"> {company.name}</h4>\n      <p className=\"CompanyDetail-description text-left\"> {company.description}</p>\n      {company.jobs.map(j => <JobCard key={j.id} job={{ ...j, companyName: undefined }} />)}\n    </div>);\n}\n\n\nexport default CompanyDetail;","/Users/seankim/Dropbox/Programming/Rithm/rithm-exercises/w9/react-jobly-frontend/src/LoginForm.js",[],"/Users/seankim/Dropbox/Programming/Rithm/rithm-exercises/w9/react-jobly-frontend/src/SignupForm.js",[],"/Users/seankim/Dropbox/Programming/Rithm/rithm-exercises/w9/react-jobly-frontend/src/CompanyList.js",[],"/Users/seankim/Dropbox/Programming/Rithm/rithm-exercises/w9/react-jobly-frontend/src/JobList.js",[],"/Users/seankim/Dropbox/Programming/Rithm/rithm-exercises/w9/react-jobly-frontend/src/ProfileForm.js",[],"/Users/seankim/Dropbox/Programming/Rithm/rithm-exercises/w9/react-jobly-frontend/src/api.js",[],"/Users/seankim/Dropbox/Programming/Rithm/rithm-exercises/w9/react-jobly-frontend/src/CompanyCard.js",["74"],"import { Link } from \"react-router-dom\";\nimport \"./CompanyCard.css\";\n\n/** CompanyCard Component\n * \n * Props:\n * - company: { company data }\n * \n * State:\n * \n * CompanyList -> CompanyCard\n*/\nfunction CompanyCard({ company }) {\n  return (\n    <Link to={`/companies/${company.handle}`} className=\"CompanyCard\">\n      <div className=\"CompanyCard-card card\">\n        <div className=\"CompanyCard-body card-body\">\n          {company.logoUrl\n            ? <img className=\"float-right\" src={company.logoUrl} alt={company.handle}/>\n            : null}\n          <h5 className=\"CompanyCard-title card-title text-left\">{company.name}</h5>\n          <p className=\"CompanyCard-description text-left\">{company.description}</p>\n        </div>\n      </div>\n    </Link>\n  );\n}\n\nexport default CompanyCard;","/Users/seankim/Dropbox/Programming/Rithm/rithm-exercises/w9/react-jobly-frontend/src/JobCard.js",[],"/Users/seankim/Dropbox/Programming/Rithm/rithm-exercises/w9/react-jobly-frontend/src/SearchForm.js",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":1,"column":35,"nodeType":"81","messageId":"82","endLine":1,"endColumn":48},{"ruleId":"83","severity":1,"message":"84","line":22,"column":6,"nodeType":"85","endLine":22,"endColumn":8,"suggestions":"86"},{"ruleId":"87","severity":1,"message":"88","line":19,"column":15,"nodeType":"89","endLine":19,"endColumn":89},"no-native-reassign",["90"],"no-negated-in-lhs",["91"],"no-unused-vars","'BrowserRouter' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'handle'. Either include it or remove the dependency array.","ArrayExpression",["92"],"jsx-a11y/img-redundant-alt","Redundant alt attribute. Screen-readers already announce `img` tags as an image. You don’t need to use the words `image`, `photo,` or `picture` (or any specified custom words) in the alt prop.","JSXOpeningElement","no-global-assign","no-unsafe-negation",{"desc":"93","fix":"94"},"Update the dependencies array to be: [handle]",{"range":"95","text":"96"},[583,585],"[handle]"]